{
	"manifest_version": 3,
	"name": "J-GLOBAL データ獲得",
	"version": "2.0",
	"description": "J-GLOBALの研究者情報を人が評価しやすくする",
	"permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],

	"content_scripts": [
	  {
		"matches": ["https://jglobal.jst.go.jp/search/*"],
		"js": ["content.js"]
	  },
	  {
		"matches": ["https://jglobal.jst.go.jp/bookmark*"],
		"js": ["content.js"]
	  }
	]

  }
