{
  "manifest_version": 3,
  "name": "J-GLOBAL Scraper",
  "version": "1.0",
  "description": "Scrapes data from J-GLOBAL pages.",
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
